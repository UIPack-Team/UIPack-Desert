{
  "namespace": "inventory_uikit",
  "cell_images": {
    "type": "panel",
    "controls": [
      {
        "img": {
          "type": "image",
          "texture": "assets/minecraft/textures/gui/container/creative_inventory/tab_items",
          "uv":[16,33],
          "uv_size":[34,35],
          "$cell_image_size|default": [17,17],
          "size": "$cell_image_size",
          "bindings": [
            {
              "binding_type": "view",
              "source_control_name": "$background_image_control_name",
              "resolve_sibling_scope": true,
              "source_property_name": "((#container_item_background = 0)or (#container_item_background = 1)or (#container_item_background = 2) or (#container_item_background = 3)or(#container_item_background = 4)or(#container_item_background = 7))",
              "target_property_name": "#visible"
            }
          ]
        }
      }
    ],
    "bindings": [
      {
        "binding_name": "#container_item_background",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name",
        "binding_condition": "visible"
      }
    ]
  },
  "top_tab@common_tabs.base_tab": {
    "$tab_unchecked_uv|default": [56, 2],
    "$tab_unchecked_uv_size|default": [56, 62],
    "$tab_checked_uv|default": [0, 64],
    "$tab_checked_uv_size|default": [54, 65],
    "size": [26, "100%+3px"],
    "$tab_content": "crafting.empty_tab_panel",
    "$radio_toggle_group": true,
    "$toggle_name": "second_tab",
    "$toggle_default_state": false,
    "$tab_panel": "inventory_uikit.tab_panel",
    "controls": [
      {
        "$tab_view_binding_name@common.toggle": {
          "$toggle_tts_name": "$top_tab_tts_name",
          "$toggle_tts_enabled_binding_type": "global",
          "$toggle_reset_on_focus_lost|default": true,
          "reset_on_focus_lost": "$toggle_reset_on_focus_lost",
          "variables": [
            {
              "requires": "($is_top_left_most_tab and not $is_top_right_most_tab)",
              "$tab_unchecked_uv": [0, 2],
              "$tab_checked_complex": "inventory_controls.topleft_tab_checked_complex"
            },
            {
              "requires": "($is_top_right_most_tab and not $is_top_left_most_tab)",
              "$tab_unchecked_uv": [112, 2],
              "$tab_checked_uv": [112, 64],
              "$tab_checked_uv_size": [56, 64],
              "$tab_checked_complex": "inventory_controls.topright_tab_checked_complex"
            }
          ],
          "controls": [
            {
              "unchecked@$tab_panel": {
                "$tab_uv": "$tab_unchecked_uv",
                "$tav_uv_size": "$tab_unchecked_uv_size",
                "$tab_state": "unchecked",
                "layer": 1
              }
            },
            {
              "unchecked_hover@$tab_panel": {
                "$tab_uv": "$tab_unchecked_uv",
                "$tav_uv_size": "$tab_unchecked_uv_size",
                "$tab_state": "unchecked_hover",
                "$is_hover_state": true,
                "layer": 1
              }
            },
            {
              "checked@$tab_panel": {
                "$tab_offset": [0, -2],
                "$tab_uv": "$tab_checked_uv",
                "$tav_uv_size": "$tab_checked_uv_size",
                "$tab_state": "checked",
                "$tab_checked_complex|default": "common.empty_panel",
                "$tab_complex": "$tab_checked_complex",
                "$is_checked_state": true,
                "layer": 20
              }
            },
            {
              "checked_hover@$tab_panel": {
                "$tab_offset": [0, -2],
                "$tab_uv": "$tab_checked_uv",
                "$tav_uv_size": "$tab_checked_uv_size",
                "$tab_state": "checked_hover",
                "$is_checked_state": true,
                "$tab_checked_complex|default": "common.empty_panel",
                "$tab_complex": "$tab_checked_complex",
                "$is_hover_state": true,
                "layer": 20
              }
            }
          ]
        }
      }
    ]
  },
  "bottom_tab@common_tabs.base_tab": {
    "$tab_unchecked_uv|default": [224, 2],
    "$tab_unchecked_uv_size|default": [56, 62],
    "$tab_checked_uv|default": [223, 75],
    "$tab_checked_uv_size|default": [56, 64],
    "size": [26, "100%+3px"],
    "$tab_content": "crafting.empty_tab_panel",
    "$radio_toggle_group": true,
    "$toggle_name": "second_tab",
    "$toggle_default_state": false,
    "$tab_panel": "inventory_uikit.tab_panel",
    "controls": [
      {
        "$tab_view_binding_name@common.toggle": {
          "$toggle_tts_name": "$top_tab_tts_name",
          "$toggle_tts_enabled_binding_type": "global",
          "$toggle_reset_on_focus_lost|default": true,
          "reset_on_focus_lost": "$toggle_reset_on_focus_lost",
          "variables": [
            {
              "requires": "($is_top_left_most_tab and not $is_top_right_most_tab)",
              "$tab_unchecked_uv": [168, 0],
              "$tab_checked_complex": "inventory_controls.bottomleft_tab_checked_complex"
            },
            {
              "requires": "($is_top_right_most_tab and not $is_top_left_most_tab)",
              "$tab_unchecked_uv": [280, 0],
              "$tab_checked_uv": [280, 75],
              "$tab_checked_complex": "inventory_controls.bottomright_tab_checked_complex"
            }
          ],
          "controls": [
            {
              "unchecked@$tab_panel": {
                "$tab_offset": [0, -0.5],
                "$tab_uv": "$tab_unchecked_uv",
                "$tav_uv_size": "$tab_unchecked_uv_size",
                "$tab_state": "unchecked",
                "layer": 1
              }
            },
            {
              "unchecked_hover@$tab_panel": {
                "$tab_offset": [0, -0.5],
                "$tab_uv": "$tab_unchecked_uv",
                "$tav_uv_size": "$tab_unchecked_uv_size",
                "$tab_state": "unchecked_hover",
                "$is_hover_state": true,
                "layer": 1
              }
            },
            {
              "checked@$tab_panel": {
                "$tab_offset": [0, 5],
                "$tab_uv": "$tab_checked_uv",
                "$tav_uv_size": "$tab_checked_uv_size",
                "$tab_state": "checked",
                "$tab_checked_complex|default": "common.empty_panel",
                "$tab_complex": "$tab_checked_complex",
                "$is_checked_state": true,
                "layer": 20
              }
            },
            {
              "checked_hover@$tab_panel": {
                "$tab_offset": [0, 5],
                "$tab_uv": "$tab_checked_uv",
                "$tav_uv_size": "$tab_checked_uv_size",
                "$tab_state": "checked_hover",
                "$is_checked_state": true,
                "$tab_checked_complex|default": "common.empty_panel",
                "$tab_complex": "$tab_checked_complex",
                "$is_hover_state": true,
                "layer": 20
              }
            }
          ]
        }
      }
    ]
  },
  "tab_panel": {
    "type": "panel",
    "$tab_content|default": "common_tabs.empty_tab_content",
    "$tab_image|default": "common_tabs.tab_image",
    "$is_top_left_most_tab|default": false,
    "$is_top_right_most_tab|default": false,
    "$tab_complex|default": "common.empty_panel",
    "controls": [
      {
        "tab_image": {
          "type": "image",
          "$tab_offset|default": [0, 0.5],
          "offset": "$tab_offset",
          "texture": "assets/minecraft/textures/gui/advancements/tabs",
          "uv": "$tab_uv",
          "uv_size": "$tav_uv_size",
          "tiled": "y",
          "tiled_scale": [0, 0.55],
          "size": ["100%+3px", "100%+3px"], // We need to explicitly set the size here.
          "layer": 2,
          "controls": [
            {
              "a@$tab_complex": {}
            }
          ]
        }
      },
      {
        "tab_content_sizer": {
          "type": "panel",
          "$tab_content_size|default": ["100% - 4px", "100% - 4px"],
          "size": "$tab_content_size",
          "controls": [
            {
              "tab_content@$tab_content": {
                "layer": 3
              }
            }
          ]
        }
      }
    ]
  },
  "items_number_test": {
    "type": "panel",
    "bindings": [
      {
        "binding_name": "#recipe_book_total_items",
        "binding_name_override": "#maximum_grid_items",
        "binding_condition": "visible",
        "binding_type": "collection",
        "binding_collection_name": "recipe_book"
      },
      {
        "binding_name": "(#recipe_book_total_items = $total_recipe_items_number)",
        "binding_name_override": "#visible",
        "binding_type": "collection",
        "binding_collection_name": "recipe_book"
      }
    ]
  },
  "common_toggle@common.toggle": {
    "size": [22, 22],
    "layer": 2,
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "$toggle_target",
        "source_property_name": "#toggle_state",
        "target_property_name": "#visible"
      }
    ]
  },
  "common_toggle_un@common_toggle": {
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "$toggle_target",
        "source_property_name": "(not #toggle_state)",
        "target_property_name": "#visible"
      }
    ]
  },

  "common_panel": {
    "type": "panel",
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "$toggle_target",
        "source_property_name": "#toggle_state",
        "target_property_name": "#visible"
      }
    ]
  },
  "common_panel_un@common_panel": {
    "type": "panel",
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "$toggle_target",
        "source_property_name": "(not #toggle_state)",
        "target_property_name": "#visible"
      }
    ]
  },
  "content_stack_panel": {
    "type": "stack_panel",
    "size": [326, 166],
    "orientation": "horizontal",
    "$content_tab_content|default": "inventory_controls.tab_content",
    "$content_tab_content_2|default": "inventory_controls.tab_content",
    "controls": [
      {
        "7_tab_content_panel@$content_tab_content": {
          "layer": 2,
          "size": [146, 166],
          "$tabc_label": "$content_tabc_lael1",
          "$scrolling_pane_control": "$content_scrolling_pane_control",
          "bindings": [
            {
              "binding_type": "global",
              "binding_name": "#is_recipe_book_layout",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "center_fold@crafting.center_fold": {
          "size": [4, 166],
          "bindings": [
            {
              "binding_type": "global",
              "binding_name": "#is_recipe_book_layout",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "survival_padding": {
          "type": "panel",
          "size": ["fill", "100%"],
          "bindings": [
            {
              "binding_type": "global",
              "binding_name": "#is_survival_layout",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "17_tab_content_panel@$content_tab_content_2": {
          "layer": 2,
          "size": [326, 166],
          "$tabc_label": "$content_tabc_lael2",
          "$scrolling_pane_control": "$content_recipe_book_scroll_panel",
          "bindings": [
            {
              "binding_type": "global",
              "binding_name": "(not #is_recipe_book_layout)",
              "binding_name_override": "#visible"
            }
          ]
        }
      }
    ]
  },
  "one_grid_content_stack_panel": {
    "type": "stack_panel",
    "size": [180, 18],
    "orientation": "horizontal",
    "$content_tab_content_2|default": "inventory_uikit.one_grid_tab_content",
    "controls": [
      {
        "0@$content_tab_content_2": {
          "layer": 3,
          "size": [36, 18],
          "$scrolling_pane_control": "$grid_1"
        }
      },
      {
        "empty": {
          "type": "panel",
          "size": [-5, 0]
        }
      },
      {
        "1@a_grid": { "$content_recipe_book_scroll_panel": "$grid_2" }
      },
      {
        "2@a_grid": { "$content_recipe_book_scroll_panel": "$grid_3" }
      },
      {
        "3@a_grid": { "$content_recipe_book_scroll_panel": "$grid_4" }
      },
      {
        "4@a_grid": { "$content_recipe_book_scroll_panel": "$grid_5" }
      },
      {
        "5@a_grid": { "$content_recipe_book_scroll_panel": "$grid_6" }
      },
      {
        "6@a_grid": { "$content_recipe_book_scroll_panel": "$grid_7" }
      },
      {
        "7@a_grid": { "$content_recipe_book_scroll_panel": "$grid_8" }
      },
      {
        "8@a_grid": { "$content_recipe_book_scroll_panel": "$grid_9" }
      }
    ]
  },
  "a_grid": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [18, 18],
    "controls": [
      {
        "empty": {
          "type": "panel",
          "size": [-13, 0]
        }
      },
      {
        "2@$content_tab_content_2": {
          "layer": 2,
          "size": [36, 18],
          "$scrolling_pane_control": "$content_recipe_book_scroll_panel"
        }
      }
    ]
  },
  "one_grid_tab_content": {
    "type": "panel",
    "layer": 1,
    "controls": [
      {
        "tab_content_search_bar_panel": {
          "type": "stack_panel",
          "size": ["100% - 12px", "100% - 12px"],
          "orientation": "vertical",

          "controls": [
            {
              "scroll_pane@$scrolling_pane_control": {
                "size": ["100%", 20] //决定网格大小
              }
            }
          ]
        }
      }
    ]
  },
  "scroll_content": {
    "type": "panel",
    "size": ["100%", "100%c"],
    "$scroll_main_panel_content|default": "common.empty_panel",
    "controls": [
      {
        "content@$scroll_main_panel_content": {
          "size": ["100%-2px", "100%c"],
          "anchor_from": "top_middle",
          "anchor_to": "top_middle"
        }
      }
    ]
  },
  "scroll_track": {
    "type": "image",
    "layer": 0,
    "offset": [0, -3.5],
    "size": ["100%", "100%-9px"],
    "texture": "textures/ui/cell_image"
  },
  "scroll_box": {
    "type": "image",
    "size": ["100%", "100% - 10px"],
    "texture": "textures/ui/ScrollBox"
  },

  "inventory_scrolling_panel@common.scrolling_panel": {
    "layer": 1,
    "$scrolling_pane_size": ["100%", "100%"],
    "$scrolling_content": "inventory_uikit.scroll_content",
    "$show_background": true,
    //"$scroll_view_stack_panel_size":["100%","100%+12px"],
    "$scroll_background_image_control": "common.empty_panel",
    "$scroll_view_port_offset": [0, 0],
    "$scroll_view_port_offset_touch": [0, 0],
    "$scroll_view_port_size": ["100%", "100%"],
    "$scroll_view_port_size_touch": ["100%", "100%"],
    "$scroll_size": [14, "100%"],
    "$scroll_track_image_control": "inventory_uikit.scroll_track",
    "$scroll_box_mouse_image_control": "inventory_uikit.scroll_box",
    "size": ["100%", "100%"],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "$scroll_view_port_size|default": ["100% - 8px", "100% -2px"],
    "$scroll_view_port_offset|default": [1, 1],
    "$scroll_view_port_size_touch|default": ["100% - 4px", "100% - 2px"],
    "$scroll_view_port_offset_touch|default": [1, 1],
    "controls": [
      {
        "scroll_mouse@common.scrolling_panel_base": {
          "size": "$scrolling_pane_size",
          "offset": "$scrolling_pane_offset"
        }
      }
    ]
  },
  "default_back_toggle@inventory_uikit.common_toggle_un": {
    "$unchecked_control": "inventory_uikit.toggle_kit",
    "$checked_control": "inventory_uikit.toggle_kit",
    "$unchecked_hover_control": "inventory_uikit.toggle_kit",
    "$checked_hover_control": "inventory_uikit.toggle_kit",
    "$unchecked_locked_control": "inventory_uikit.toggle_kit",
    "$unchecked_locked_hover_control": "inventory_uikit.toggle_kit",
    "$checked_locked_control": "inventory_uikit.toggle_kit",
    "$checked_locked_hover_control": "inventory_uikit.toggle_kit",
    "$toggle_target": "inventory_uikit.default_next_toggle"
  },

  "default_next_toggle@common.toggle": {
    "size": [22, 22],
    "layer": 2,
    "$toggle_binding_type": "global",
    "$tab_view_binding_name": "search_tab_toggle",
    "$toggle_group_forced_index": "$search_index",
    "$radio_toggle_group": false,
    "$toggle_default_state": false,
    "$toggle_group_default_selected": "$construction_index",

    "$toggle_name": "navigation_tab",

    "$unchecked_control": "inventory_uikit.toggle_kit",
    "$checked_control": "inventory_uikit.toggle_kit",
    "$unchecked_hover_control": "inventory_uikit.hover_toggle_kit",
    "$checked_hover_control": "inventory_uikit.hover_toggle_kit",
    "$unchecked_locked_control": "inventory_uikit.toggle_kit",
    "$unchecked_locked_hover_control": "inventory_uikit.toggle_kit",
    "$checked_locked_control": "inventory_uikit.toggle_kit",
    "$checked_locked_hover_control": "inventory_uikit.toggle_kit",

    "$unchecked_toggle_texture": "textures/ui/button_borderless_light",
    "$toggle_img": "",
    "$complex_imgv": true,
    "$complex_img_1": "textures/ui/arrow_r_default",
    "$complex_img_2": "textures/ui/arrow_l_default",
    "$border_color|default": "$light_border_default_color"
  },

  "next_toggle@inventory_uikit.common_toggle": {
    "$unchecked_control": "inventory_uikit.toggle_kit",
    "$checked_control": "inventory_uikit.toggle_kit",
    "$unchecked_hover_control": "inventory_uikit.toggle_kit",
    "$checked_hover_control": "inventory_uikit.toggle_kit",
    "$unchecked_locked_control": "inventory_uikit.toggle_kit",
    "$unchecked_locked_hover_control": "inventory_uikit.toggle_kit",
    "$checked_locked_control": "inventory_uikit.toggle_kit",
    "$checked_locked_hover_control": "inventory_uikit.toggle_kit",
    "$tab_view_binding_name": "third",
    "$toggle_group_forced_index": 3,
    "$toggle_img": "textures/ui/arrow_r_default",
    "$toggle_target": "inventory_uikit.default_next_toggle"
  },
  "toggle_kit@common_buttons.new_ui_button_panel": {
    "$unchecked_toggle_texture|default": "textures/ui/disabledButtonNoBorder",
    "$toggle_texture|default": "",
    "$toggle_img|default": "textures/ui/arrow_l_default",
    "$border_color|default": "$light_border_locked_color",
    "$button_color|default": [1, 1, 1],
    "$complex_img_1|default": "",
    "$complex_img_2|default": "",
    "$complex_imgv|default": false,
    "controls": [
      {
        "unchecked_texture": {
          "type": "image",
          "texture": "$unchecked_toggle_texture",
          "size": ["100% - 2px", "100% - 2px"],
          "anchor_from": "center",
          "anchor_to": "center",
          "color": "$button_color",
          "layer": 1
        }
      },
      {
        "texture": {
          "type": "image",
          "texture": "$toggle_texture",
          "size": ["100% - 2px", "100% - 2px"],
          "anchor_from": "center",
          "anchor_to": "center",
          "color": "$button_color",
          "layer": 2
        }
      },
      {
        "button_content": {
          "type": "panel",
          "size": ["100% - 6px", "100% - 6px"],
          "layer": 3,
          "controls": [
            {
              "img": {
                "type": "image",
                "anchor_from": "center",
                "anchor_to": "center",
                "texture": "$toggle_img",
                "size": [10, 10]
              }
            },
            {
              "img_1@inventory_uikit.common_panel_un": {
                "type": "image",
                "anchor_from": "center",
                "anchor_to": "center",
                "texture": "$complex_img_1",
                "visible": "$complex_imgv",
                "size": [10, 10],
                "$toggle_target": "inventory_uikit.default_next_toggle"
              }
            },
            {
              "img_2@inventory_uikit.common_panel": {
                "type": "image",
                "anchor_from": "center",
                "anchor_to": "center",
                "texture": "$complex_img_2",
                "visible": "$complex_imgv",
                "size": [10, 10],
                "$toggle_target": "inventory_uikit.default_next_toggle"
              }
            }
          ]
        }
      },
      {
        "border@common_buttons.focus_border": {
          "color": "$border_color",
          "layer": 3
        }
      }
    ]
  },
  "hover_toggle_kit@toggle_kit": {
    "$toggle_texture|default": "textures/ui/button_borderless_lighthover",
    "$toggle_img|default": "textures/ui/arrow_l_default",
    "$border_color|default": "$light_border_hover_color"
  }
}
