    "duration": 0.4,
    "from": [0, 0],
    "to": [0, 200],
    "next": "@worldtemplates_uikit.anim_1",
    "play_event": "button.more_custom_templates"
  },
  "anim_1": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": 0.4,
    "from": [0, 0],
    "to": [0, 0]
  },
  "world_templates_picker_content": {
    "type": "panel",
    "controls": [
      {
        "common_panel@screen_uikit.uikit_screen": {
          "anims": ["@worldtemplates_uikit.anim_0"],
