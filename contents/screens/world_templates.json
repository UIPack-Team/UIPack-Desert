      {
        "padding_1": {
          "type": "panel",
          "size": [-421, 0]
        }
      },
      {
        "local_item_2@ui.modern_white_content_button": {
          "size": "$grid_size_0",
          "$button_content": "worldtemplates_uikit.world_template_content_panel",
