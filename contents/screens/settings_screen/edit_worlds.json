{
  "namespace": "modern_edit_worlds",
  "edit_worlds_contents@modern_settings.default_settings_screen": {
    "$left_title": "modern_edit_worlds.edit_title",
    "$extra_panel": "modern_settings.re_and_be",
    "$uipack_is_world_edit": false,
    "$screen_type": "screen_uikit.include_menu_screen"
  },

  "edit_title": {
    "type": "stack_panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "size": [200, 20],
    "offset": [2, 6],
    "controls": [
      {
        "button": {
          "type": "panel",
          "size": [16, 10],
          "controls": [
            {
              "button": {
                "type": "image",
                "offset": [-2, -4],
                "size": [8, 8],
                "texture": "textures/ui/ui_exit_button"
              }
            },
            {
              "border": {
                "type": "image",
                "offset": [-2, -4],
                "size": [8, 8],
                "texture": "textures/ui/ui_exit_focus_border",
                "color": [0, 0, 0]
              }
            },
            {
              "seed": {
                "type": "stack_panel",
                "offset": [6, -2],
                "orientation": "horizontal",
                "size": ["fill", 12],
                "controls": [
                  {
                    "1": {
                      "type": "label",
                      "layer": 2,
                      "font_scale_factor": 1,
                      "color": "white",
                      "text": "Seed : ",
                      "size": ["default", 12]
                    }
                  },
                  {
                    "seed": {
                      "type": "label",
                      "layer": 2,
                      "font_scale_factor": 1,
                      "color": "white",
                      "text": "#world_seed",
                      "size": ["default", 12],
                      "bindings": [
                        {
                          "binding_name": "#world_seed"
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "line_two": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [127, 12],
          "controls": [
            {
              "padding": { "type": "panel", "size": [2, 0] }
            },
            {
              "create_title": {
                "type": "label",
                "layer": 2,
                "font_scale_factor": 1.4,
                "color": "white",
                "text": "#world_name",
                "size": ["default", 12],
                "bindings": [
                  {
                    "binding_name": "#world_name"
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  }
}
