  "两栏文本": {
    "type": "stack_panel",
    "size": ["100%", 10],
    "orientation": "horizontal",
    "controls": [
      {
        "0": {
          "type": "label",
          "$option_label_1|default": "",
          "color": "$main_header_text_color",
          "size": ["default", 10],
          "layer": 1,
          "text": "$option_label_1",
          "locked_alpha": 0.5
        }
      },
      {
        "1": {
          "type": "label",
          "$option_label_2|default": "",
          "color": "$main_header_text_color",
          "size": ["default", 10],
          "layer": 1,
          "text": "$option_label_2"
        }
      },
      {
        "2": {
          "type": "label",
          "$option_label_3|default": "",
          "color": "$main_header_text_color",
          "size": ["default", 10],
          "layer": 1,
          "text": "$option_label_3"
        }
      }
