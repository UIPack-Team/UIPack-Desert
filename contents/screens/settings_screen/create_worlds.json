    "$button_content": "modern_settings.toggle_button_control",
    "$glyph_size": [22, 12],
    "$button_text": "Experiement",
    "$toggle_property_bag": {
      "#panel_title": "createWorldScreen.multiplayer.settings"
    }
  },
  "create_random_worlds_contents@modern_settings.default_settings_screen": {
    "$left_title": "modern_create_worlds.create_title",
    "$uipack_is_world_edit": true,
    "$screen_type": "screen_uikit.include_menu_screen"
  },
  "create_worlds_from_templates_contents@modern_settings.default_settings_screen": {
    "$uipack_is_world_edit": true,
    "$screen_type": "screen_uikit.include_menu_screen"
