  "create_title": {
    "type": "stack_panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "size": [200, 20],
    "offset": [2, 6],
    "controls": [
      {
        "button": {
          "type": "panel",
          "size": [16, 10],
          "controls": [
            {
              "button": {
                "type": "image",
                "offset": [-2, -4],
                "size": [8, 8],
                "texture": "textures/ui/ui_exit_button"
              }
            },
            {
              "border": {
                "type": "image",
                "offset": [-2, -4],
                "size": [8, 8],
                "texture": "textures/ui/ui_exit_focus_border",
                "color": [0, 0, 0]
              }
            }
          ]
        }
      },
      {
        "line_two": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [127, 12],
          "controls": [
            {
              "padding": { "type": "panel", "size": [2, 0] }
            },
            {
              "create_title": {
                "type": "label",
                "layer": 2,
                "font_scale_factor": 1.4,
                "color": "white",
                "text": "ui_kit.NewWorld",
                "size": ["default", 12]
              }
            },
            {
              "padding2": {
                "type": "panel",
                "size": ["fill", 0]
              }
            },
            {
              "panel_1": {
                "type": "panel",
