    "$button_text": "World Edit",
    "$toggle_property_bag": {
      "#panel_title": "createWorldScreen.game.settings"
    }
  },
  "cheat_button@ui.Snowfirewolf_toggle_button": {
    "$toggle_view_binding_name": "world_debug_button_toggle",
    "$default_glyph_color": [0.075, 0.075, 0.075],
    "$glyph_texture": "textures/ui/debug_glyph_color",
    "$button_content": "modern_settings.toggle_button_control",
    "$glyph_size": [22, 12],
    "$button_text": "Cheat Options",
    "$toggle_property_bag": {
      "#panel_title": "createWorldScreen.multiplayer.settings"
    }
  },
  "experiment_button@ui.Snowfirewolf_toggle_button": {
    "$toggle_view_binding_name": "multiplayer_button_toggle",
    "$default_glyph_color": [0.075, 0.075, 0.075],
