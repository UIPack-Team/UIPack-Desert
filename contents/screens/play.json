{
  "namespace": "play_uikit",

  "play_screen_content": {
    "$with_tab_contents": "play_uikit.play_contents",
    "type": "panel",
    "controls": [
      {
        "common_panel@screen_uikit.uikit_screen": {
          "$screen_type": "screen_uikit.include_tabs_screen",
          "$tab_navigation": "play_uikit.tab_navigation_panel_layout"
        }
      },
      {
        "back@screen_uikit.bgback": {}
      }
    ]
  },
  "default_back_toggle@common.toggle": {
    "size": [400, 32],
    "layer": 20,
    "$unchecked_control": "play_uikit.img",
    "$checked_control": "play_uikit.img",
    "$unchecked_hover_control": "play_uikit.img",
    "$checked_hover_control": "play_uikit.img",
    "$unchecked_locked_control": "play_uikit.img",
    "$unchecked_locked_hover_control": "play_uikit.img",
    "$checked_locked_control": "play_uikit.img",
    "$checked_locked_hover_control": "play_uikit.img"
  },
  "img": {
    "type": "image",
    "texture": "textures/ui/bg",
    "size": ["100%", "100%"],
    "alpha": 0.1
  },
  "play_contents": {
    "type": "panel",
    "controls": [
      {
        "6_grids@play_uikit_showing.scroll_panel": {
          "size": ["100%", "100%-12px"],
          "bindings": [
            {
              "binding_type": "view",
              "source_control_name": "play_uikit.default_back_toggle",
              "source_property_name": "(not #toggle_state)",
              "target_property_name": "#visible"
            }
          ]
        }
      },
      {
        "tab_contents": {
          "type": "panel",
          "bindings": [
            {
              "binding_type": "view",
              "source_control_name": "play_uikit.default_back_toggle",
              "source_property_name": "#toggle_state",
              "target_property_name": "#visible"
            }
          ],
          "controls": [
            {
              "worlds_tab_content@common_tab_content_panel": {
                "$visible_bind_toggle_source_control": "world_navigation_tab_toggle",
                "$scroll_tab_content": "play_uikit.worlds_scroll_content"
              }
            },
            {
              "friends_tab_contentl@common_tab_content_panel": {
                "$visible_bind_toggle_source_control": "friends_navigation_tab_toggle",
                "$scroll_tab_content": "play_uikit.friends_scroll_content"
              }
            },
            {
              "server_tab_content_panel@common_tab_content_panel": {
                "$visible_bind_toggle_source_control": "server_navigation_tab_toggle",
                "$scroll_tab_content": "play_uikit.server_scroll_content"
              }
            }
          ]
        }
      },
      {
        "popup_dialog_factory": {
          "type": "factory",
          "control_ids": {
            "join_by_code": "@play.popup_dialog_join_by_code",
            "high_ping_warning": "@play.popup_dialog_high_ping"
          }
        }
      }
    ]
  },
  "worlds_tab_icon": {
    "type": "image",
    "texture": "textures/ui/worldsIcon",
    "size": [16, 17]
  },

  "friends_tab_icon": {
    "type": "image",
    "texture": "textures/ui/FriendsIcon"
  },

  "server_tab_icon": {
    "type": "image",
    "texture": "textures/ui/servers"
  },
  "tab_navigation_panel_layout": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "anchor_to": "center",
    "anchor_from": "center",
    "offset": [0, 0],
    "size": ["100%", 32],
    "controls": [
      {
        "navigation_tabs": {
          "type": "panel",
          "size": ["100%", "100%"],
          "controls": [
            {
              "contents": {
                "type": "stack_panel",
                "orientation": "horizontal",
                "size": ["100%c", "32px"],
                "controls": [
                  {
                    "world_navigation_tab@play_uikit.top_tab": {
                      "$is_left_most_tab": true,
                      "$tab_view_binding_name": "world_navigation_tab_toggle",
                      "$tab_icon": "worlds_tab_icon",
                      "$count_text_name": "#world_item_count",
                      "$tab_text_name": "selectWorld.tab.worlds",
                      "$toggle_group_forced_index": 0,
                      "$tab_content": "play_uikit.common_navigation_tab_content",
                      "size": ["32px", "100%"]
                    }
                  },
                  {
                    "space_0": {
                      "type": "panel",
                      "size": ["5px", "100%"]
                    }
                  },
                  {
                    "friends_navigation_tab@play_uikit.top_tab": {
                      "$tab_view_binding_name": "friends_navigation_tab_toggle",
                      "$tab_icon": "friends_tab_icon",
                      "$count_text_name": "#friend_world_item_count",
                      "$tab_text_name": "selectWorld.tab.friends",
                      "$toggle_group_forced_index": 1,
                      "$tab_content": "play_uikit.common_navigation_tab_content",
                      "size": ["32px", "100%"]
                    }
                  },
                  {
                    "space_1": {
                      "type": "panel",
                      "size": ["5px", "100%"]
                    }
                  },
                  {
                    "server_navigation_tab@play_uikit.top_tab": {
                      "$tab_view_binding_name": "server_navigation_tab_toggle",
                      "$tab_icon": "server_tab_icon",
                      "$count_text_name": "#server_world_item_count",
                      "$tab_text_name": "selectWorld.tab.thirdParty",
                      "$toggle_group_forced_index": 2,
                      "$tab_content": "play_uikit.common_navigation_tab_content",
                      "size": ["32px", "100%"]
                    }
                  }
                ]
              }
            },
            {
              "test@settings_common.option_generic": {
                "$control_name": "play_uikit.default_back_toggle",
                "layer": 8,
                "size": [400, 32],
                "bindings": [
                  {
                    "binding_type": "view",
                    "source_control_name": "play_uikit.default_back_toggle",
                    "source_property_name": "(not #toggle_state)",
                    "target_property_name": "#visible"
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },
  "top_tab@play_uikit.tab": {
    "$toggle_focus_enabled": false,
    "$radio_toggle_group": true,
    "$toggle_name": "navigation_tab",
    "$toggle_group_default_selected": "$default_tab"
  },

  "tab@play_uikit.play_tab": {
    "$unchecked_glyph_color": [0.3, 0.3, 0.3],
    "$unchecked_hover_glyph_color": [1.0, 1.0, 1.0],
    "$unchecked_default_image": "textures/ui/tabs/TabTopBack",
    "$unchecked_hover_image": "textures/ui/tabs/TabTopHover",
    "$unchecked_locked_image": "textures/ui/tabs/TabTopBack",
    "$unchecked_locked_hover_image": "textures/ui/tabs/TabTopBack",
    "$checked_glyph_color": [1.0, 1.0, 1.0],
    "$checked_hover_glyph_color": [1.0, 1.0, 1.0],
    "$checked_default_image": "textures/ui/tabs/TabTopFront",
    "$checked_hover_image": "textures/ui/tabs/TabTopHover",
    "$checked_locked_image": "textures//ui/tabs/TabTopFront",
    "$checked_locked_hover_image": "textures//ui/tabs/TabTopFront"
  },

  "play_tab": {
    "type": "panel",
    "$unchecked_text_color": "$tab_unchecked_text_color",
    "$unchecked_hover_text_color": "$tab_unchecked_hover_text_color",
    "$unchecked_locked_text_color": "$tab_unchecked_locked_text_color",
    "$unchecked_locked_hover_text_color": "$tab_unchecked_locked_hover_text_color",
    "$checked_text_color": "$tab_checked_text_color",
    "$checked_hover_text_color": "$tab_checked_hover_text_color",
    "$checked_locked_text_color": "$tab_checked_locked_text_color",
    "$checked_locked_hover_text_color": "$tab_checked_locked_hover_text_color",

    "$unchecked_light_button_hover_text_color": "$tab_secondary_unchecked_text_color",
    "$unchecked_hover_light_button_hover_text_color": "$tab_secondary_unchecked_hover_text_color",
    "$unchecked_locked_light_button_hover_text_color": "$tab_secondary_unchecked_locked_text_color",
    "$unchecked_locked_hover_light_button_hover_text_color": "$tab_secondary_unchecked_locked_hover_text_color",
    "$checked_light_button_hover_text_color": "$tab_secondary_checked_text_color",
    "$checked_hover_light_button_hover_text_color": "$tab_secondary_checked_hover_text_color",
    "$checked_locked_light_button_hover_text_color": "$tab_secondary_checked_locked_text_color",
    "$checked_locked_hover_light_button_hover_text_color": "$tab_secondary_checked_locked_hover_text_color",

    "$unchecked_glyph_color": "$tab_unchecked_glyph_color",
    "$unchecked_hover_glyph_color": "$tab_unchecked_hover_glyph_color",
    "$unchecked_locked_glyph_color": "$tab_unchecked_locked_glyph_color",
    "$unchecked_locked_hover_glyph_color": "$tab_unchecked_locked_hover_glyph_color",
    "$checked_glyph_color": "$tab_checked_glyph_color",
    "$checked_hover_glyph_color": "$tab_checked_hover_glyph_color",
    "$checked_locked_glyph_color": "$tab_checked_locked_glyph_color",
    "$checked_locked_hover_glyph_color": "$tab_checked_locked_hover_glyph_color",

    "$tab_view_binding_name|default": "tab",
    "$tab_panel|default": "common_tabs.tab_panel",

    "controls": [
      {
        "$tab_view_binding_name@common.toggle": {
          "controls": [
            {
              "unchecked@$tab_panel": {
                "$text_color": "$unchecked_text_color",
                "$light_button_hover_text_color": "$unchecked_light_button_hover_text_color",
                "$glyph_color": "$unchecked_glyph_color",
                "$tab_texture": "$unchecked_default_image",
                "$tab_state": "unchecked",
                "layer": 1
              }
            },
            {
              "unchecked_hover@$tab_panel": {
                "$text_color": "$unchecked_hover_text_color",
                "$light_button_hover_text_color": "$unchecked_hover_light_button_hover_text_color",
                "$glyph_color": "$unchecked_hover_glyph_color",
                "$tab_texture": "$unchecked_hover_image",
                "$tab_state": "unchecked_hover",
                "layer": 1
              }
            },
            {
              "unchecked_locked@$tab_panel": {
                "$text_color": "$unchecked_locked_text_color",
                "$light_button_hover_text_color": "$unchecked_locked_light_button_hover_text_color",
                "$glyph_color": "$unchecked_locked_glyph_color",
                "$tab_texture": "$unchecked_locked_image",
                "$tab_state": "unchecked_locked",
                "layer": 1
              }
            },
            {
              "unchecked_locked_hover@$tab_panel": {
                "$text_color": "$unchecked_locked_hover_text_color",
                "$light_button_hover_text_color": "$unchecked_locked_hover_light_button_hover_text_color",
                "$glyph_color": "$unchecked_locked_hover_glyph_color",
                "$tab_texture": "$unchecked_locked_hover_image",
                "$tab_state": "unchecked_locked_hover",
                "layer": 1
              }
            },
            {
              "checked@$tab_panel": {
                "$text_color": "$checked_text_color",
                "$light_button_hover_text_color": "$checked_light_button_hover_text_color",
                "$glyph_color": "$checked_glyph_color",
                "$tab_texture": "$checked_default_image",
                "$tab_state": "checked",
                "layer": 2
              }
            },
            {
              "checked_hover@$tab_panel": {
                "$text_color": "$checked_hover_text_color",
                "$light_button_hover_text_color": "$checked_hover_light_button_hover_text_color",
                "$glyph_color": "$checked_hover_glyph_color",
                "$tab_texture": "$checked_hover_image",
                "$tab_state": "checked_hover",
                "layer": 2
              }
            },
            {
              "checked_locked@$tab_panel": {
                "$text_color": "$checked_locked_text_color",
                "$light_button_hover_text_color": "$checked_locked_light_button_hover_text_color",
                "$glyph_color": "$checked_locked_glyph_color",
                "$tab_texture": "$checked_locked_image",
                "$tab_state": "checked_locked",
                "layer": 2
              }
            },
            {
              "checked_locked_hover@$tab_panel": {
                "$text_color": "$checked_locked_hover_text_color",
                "$light_button_hover_text_color": "$checked_locked_hover_light_button_hover_text_color",
                "$glyph_color": "$checked_locked_hover_glyph_color",
                "$tab_texture": "$checked_locked_hover_image",
                "$tab_state": "checked_locked_hover",
                "layer": 2
              }
            }
          ]
        }
      }
    ]
  },
  "common_navigation_tab_content": {
    "type": "stack_panel",
    "size": ["100%", "100%c"],
    "orientation": "vertical",
    "controls": [
      {
        "icon_section": {
          "type": "panel",
          "size": ["100%", "16px"],
          "controls": [
            {
              "icon@$tab_icon": {
                "size": ["16px", "16px"],
                "color": "$glyph_color",
                "layer": 2
              }
            }
          ]
        }
      }
    ]
  },
  "common_tab_content_panel": {
    "type": "panel",
    "controls": [
      {
        "content@$scroll_tab_content": {
          "layer": 2,
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle",
          "size": ["100%", "100%"],
          "offset": [0, 0]
        }
      }
    ],
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "$visible_bind_toggle_source_control",
        "source_property_name": "#toggle_state",
        "target_property_name": "#visible"
      }
    ]
  },
  "worlds_scroll_content@play.common_content": {
    "controls": [
      {
        "scrolling_panel@common.scrolling_panel": {
          "size": ["100%", "100%"],
          "$show_background": false,
          "offset": [0, 0],
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle",
          "layer": 1,
          "$scrolling_content": "play_uikit.worlds_scroll_panel"
        }
      }
    ]
  },
  "friends_scroll_content@play.common_content": {
    "controls": [
      {
        "scrolling_panel@common.scrolling_panel": {
          "size": ["100%", "100%"],
          "$show_background": false,
          "offset": [0, 0],
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle",
          "layer": 1,
          "$scrolling_content": "play_uikit.friends_scroll_panel"
        }
      }
    ]
  },
  "server_scroll_content@play.common_content": {
    "controls": [
      {
        "label": {
          "type": "label",
          "color": "$dark_button_default_text_color",
          "offset": [4, 2],
          "size": ["100%", "default"],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "layer": 1,
          "shadow": false,
          "text": "thirdPartyWorld.Featured"
        }
      },
      {
        "scrolling_panel@common.scrolling_panel": {
          "size": ["50%-6px", "100%"],
          "$show_background": false,
          "offset": [2, 12],
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "layer": 1,
          "$scrolling_content": "play_uikit.server_scroll_panel"
        }
      },
      {
        "bg_3": {
          "type": "image",
          "layer": 0,
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "keep_ratio": false,
          "texture": "textures/ui/Black",
          "size": ["50%+2px", "100%"]
        }
      },
      {
        "third_party_scrolling_panel@play.scrolling_offsets": {
          "size": ["50%", "100%+1px"],
          "offset": [0, 0],
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "layer": 8,
          "$scrolling_content": "play.third_party_server_info_panel",
          "bindings": [
            {
              "binding_name": "#is_third_party_server_selected",
              "binding_name_override": "#visible"
            }
          ]
        }
      }
    ]
  },
  "import_icon": {
    "type": "image",
    "color": "$text_color",
    "texture": "textures/ui/import",
    "size": [16, 16]
  },
  "worlds_scroll_panel@play.common_scroll_pane": {
    "controls": [
      {
        "worlds_stack_panel": {
          "type": "stack_panel",
          "orientation": "vertical",
          "size": ["100%", "100%c"],
          "controls": [
            {
              "padding_0": {
                "type": "panel",
                "size": ["100%", 2]
              }
            },
            {
              "header_button": {
                "type": "stack_panel",
                "orientation": "horizontal",
                "layer": 1,
                "size": ["100%", 30],
                "controls": [
                  {
                    "-0": {
                      "type": "panel",
                      "size": [1, 0]
                    }
                  },
                  {
                    "primary@ui.Snowfirewolf_button": {
                      "variables": [
                        {
                          "requires": "($local_world_upload)",
                          "$pressed_button_name": "button.new_world_upload"
                        },
                        {
                          "requires": "(not $local_world_upload)",
                          "$pressed_button_name": "button.menu_local_world_create"
                        }
                      ],
                      "$button_text": "Creat a New World",
                      "size": ["fill", "100%"],
                      "anchor_from": "top_left",
                      "anchor_to": "top_left",
                      "layer": 1
                    }
                  },
                  {
                    "-1": {
                      "type": "panel",
                      "size": [-2, 0]
                    }
                  },
                  {
                    "secondary@ui.modern_content_button": {
                      "variables": [
                        {
                          "requires": "($local_world_upload)",
                          "$pressed_button_name": "button.archived_world_upload"
                        },
                        {
                          "requires": "(not $local_world_upload)",
                          "$pressed_button_name": "button.menu_import_level"
                        }
                      ],
                      "size": [29, "100%"],
                      "$button_content": "play_uikit.import_icon",
                      "anchor_from": "top_right",
                      "anchor_to": "top_right",
                      "layer": 1
                    }
                  }
                ]
              }
            },
            {
              "padding_1": {
                "type": "panel",
                "size": ["100%", 3]
              }
            },
            {
              "gridd@play.world_item_grid_base": {
                "grid_item_template": "play_uikit.local_world_item",
                "grid_dimension_binding": "#local_world_item_grid_dimension",
                "collection_name": "local_worlds",
                "bindings": [
                  {
                    "binding_name": "#local_world_item_grid_dimension"
                  }
                ]
              }
            },
            {
              "bottom": {
                "type": "panel",
                "size": ["100%", 3]
              }
            }
          ]
        }
      }
    ]
  },
  "local_world_item": {
    "type": "stack_panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "layer": 1,
    "size": ["100%", 29],
    "orientation": "horizontal",
    "$local_world_edit_button_visible|default": true,
    "controls": [
      {
        "header_button_panel": {
          "type": "panel",
          "controls": [
            {
              "buttons": {
                "type": "stack_panel",
                "orientation": "horizontal",
                "size": ["100%", "100%"],
                "$secondary_visible|default": true,
                "controls": [
                  {
                    "-0": {
                      "type": "panel",
                      "size": [1, 0]
                    }
                  },
                  {
                    "primary_panel": {
                      "type": "panel",
                      "size": ["fill", "100%"],
                      "controls": [
                        {
                          "primary@$primary_header_button": {
                            "size": ["100% + 1px", "100%"],
                            "anchor_from": "top_left",
                            "anchor_to": "top_left",
                            "layer": 1
                          }
                        }
                      ]
                    }
                  },
                  {
                    "secondary@$secondary_header_button": {
                      "visible": "$secondary_visible",
                      "anchor_from": "top_right",
                      "anchor_to": "top_right",
                      "layer": 1
                    }
                  }
                ]
              }
            }
          ],
          "layer": 1,
          "size": ["100%", "100% + 1px"],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "$primary_header_button": "play_uikit.local_world_item_button",
          "$secondary_header_button": "play_uikit.local_world_edit_button",
          "$secondary_visible": "$local_world_edit_button_visible"
        }
      }
    ]
  },
  "local_world_item_button@ui.modern_content_button": {
    "$pressed_button_name": "button.menu_start_local_world",
    "$worlds_collection_name": "local_worlds",
    "$world_image_binding_name": "#local_world_image",
    "$world_supports_realmsplus_overlay": true,
    "$world_texture_source_binding_name": "#local_world_texture_source",
    "$world_text_panel_control": "play.local_world_text_panel",
    "$button_content": "play_uikit.world_content_panel",
    "bindings": [
      {
        "binding_type": "collection_details",
        "binding_collection_name": "local_worlds"
      },
      {
        "binding_type": "collection",
        "binding_name": "#world_button_focus_identifier",
        "binding_name_override": "#focus_identifier",
        "binding_collection_name": "local_worlds"
      }
    ]
  },
  "local_world_edit_button@ui.modern_content_button": {
    "size": ["100%y - 1px", "100%"],
    "$pressed_button_name": "button.menu_local_world_item_edit",
    "$button_content": "play.edit_icon",
    "layer": 2,
    "bindings": [
      {
        "binding_type": "collection_details",
        "binding_collection_name": "local_worlds"
      }
    ]
  },
  "world_content_panel": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": ["100%", "100%"],

    "controls": [
      {
        "world_screenshot@play_uikit.collection_world_screenshot": {
          "$collection_binding_name": "$worlds_collection_name",
          "$image_binding_name": "$world_image_binding_name",
          "$supports_realmsplus_overlay": "$world_supports_realmsplus_overlay",
          "$texture_source_binding_name": "$world_texture_source_binding_name"
        }
      },
      {
        "world_text_panel@$world_text_panel_control": {
          "size": ["fill", "100%"]
        }
      }
    ]
  },
  "collection_world_screenshot": {
    "type": "image",
    "layer": 1,
    "$fit_to_width": "$fit_image_to_width",
    "force_texture_reload": true,
    "size": ["173.91%y", "100% - 2px"],
    "offset": [1, 1],
    "fill": true,
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "bindings": [
      {
        "binding_name": "$image_binding_name",
        "binding_name_override": "#texture",
        "binding_type": "collection",
        "binding_collection_name": "$collection_binding_name"
      },
      {
        "binding_name": "$texture_source_binding_name",
        "binding_name_override": "#texture_file_system",
        "binding_type": "collection",
        "binding_collection_name": "$collection_binding_name"
      }
    ],
    "controls": [
      {
        "focus_border": {
          "$focus_border_size|default": ["100%+2px", "100%+2px"],
          "layer": -1,
          "color": "$glyph_color",
          "size": "$focus_border_size",
